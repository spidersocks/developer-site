{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/800m-calculator",
      "destination": "/en/800m-calculator",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/en/800m-calculator",
      "destination": "/calculator-en.html"
    },
    {
      "source": "/en/800m-calculator/:path*",
      "destination": "/calculator-en.html"
    },
    {
      "source": "/zh/800m-calculator",
      "destination": "/calculator-zh.html"
    },
    {
      "source": "/zh/800m-calculator/:path*",
      "destination": "/calculator-zh.html"
    },
    {
      "source": "/poke-team-predictor",
      "destination": "/poke-team-predictor.html"
    },
    {
      "source": "/poke-team-predictor/:path*",
      "destination": "/poke-team-predictor.html"
    },
    {
      "source": "/podcast-project",
      "destination": "/podcast-project.html"
    },
    {
      "source": "/podcast-project/:path*",
      "destination": "/podcast-project.html"
    },
    {
      "source": "/medical-scribe",
      "destination": "/medical-scribe.html"
    },
    {
      "source": "/medical-scribe/:path*",
      "destination": "/medical-scribe.html"
    },
    {
      "source": "/little-scholars-bot",
      "destination": "/little-scholars-bot.html"
    },
    {
      "source": "/little-scholars-bot/:path*",
      "destination": "/little-scholars-bot.html"
    },

    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}