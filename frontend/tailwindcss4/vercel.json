{
  "redirects": [
    {
      "source": "/800m-calculator",
      "destination": "/en/800m-calculator",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/800m-calculator",
      "destination": "/calculator.html"
    },
    {
      "source": "/800m-calculator/**",
      "destination": "/calculator.html"
    },
    {
      "source": "/poke-team-predictor",
      "destination": "/poke-team-predictor.html"
    },
    {
      "source": "/poke-team-predictor/**",
      "destination": "/poke-team-predictor.html"
    },
    {
      "source": "/podcast-project",
      "destination": "/podcast-project.html"
    },
    {
      "source": "/podcast-project/**",
      "destination": "/podcast-project.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NPM_FLAGS": "--legacy-peer-deps"
    }
  }
}